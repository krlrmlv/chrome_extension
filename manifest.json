{
  "manifest_version": 2,
  "name": "GetInfo",
  "description": "My first extension that shows data from window.navigator to the user",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "coding.png"
  }
}